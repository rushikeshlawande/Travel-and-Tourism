create table account(username varchar(30),name varchar(30),password varchar(30), security varchar(30),answer varchar(30),mobileNo varchar(13),emailID varchar(30), primary key(username));

create table customer(username varchar(30),id varchar(20), idnumber varchar(20),name varchar(30),gender varchar(10),country varchar(20),address varchar(40),mobileNo varchar(13),emailID varchar(30), primary key(username));

create table bookpackage(username varchar(30),package varchar(30),persons varchar(5),id varchar(20), idnumber varchar(20), mobileNo varchar(12), cost varchar(20));

create table bookhotel(username varchar(30),hotel varchar(30),persons varchar(5), days varchar(4), ac varchar(10), food varchar(10),id varchar(20), idnumber varchar(20), mobileNo varchar(12), cost varchar(20));

create table hotel(name varchar(40), cost_per_day varchar(20), food_charges varchar(20), ac_charges varchar(20));
